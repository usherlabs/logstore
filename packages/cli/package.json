{
	"name": "@logsn/cli",
	"version": "0.0.10",
	"author": "Ryan Soury <ryan@usher.so>",
	"license": "GPL-3.0",
	"description": "The Log Store Network CLI",
	"main": "./dist/src/index.js",
	"module": "./dist/src/index.js",
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/usherlabs/logstore.git",
		"directory": "packages/cli"
	},
	"scripts": {
		"build": "del dist && tsx build.ts",
		"bin": "del out && pkg --no-bytecode --public-packages '*' --output out/logstore package.json",
		"build:binaries": "run-s build bin",
		"start": "node ./dist/src/index.js",
		"format": "prettier --write .",
		"start:fresh": "npm-run-all -s build start",
		"ts": "tsc --noEmit",
		"lint": "eslint -c ./.eslintrc ./src/**/*.ts"
	},
	"bin": {
		"logstore": "./dist/bin/logstore-cli.js"
	},
	"files": [
		"./dist"
	],
	"dependencies": {
		"@commander-js/extra-typings": "^11.0.0",
		"@inquirer/prompts": "^3.3.0",
		"@logsn/client": "workspace:*",
		"@logsn/shared": "workspace:*",
		"chalk": "^4.1.2",
		"commander": "^11.0.0",
		"decimal.js": "^10.4.3",
		"ethers": "^5.7.0",
		"inquirer": "^8.1.1",
		"jsonfile": "^6.1.0",
		"lodash": "^4.17.21",
		"mkdirp": "^3.0.1",
		"rxjs": "8.0.0-alpha.12",
		"tslog": "^4.8.2"
	},
	"devDependencies": {
		"@esbuild-plugins/node-resolve": "^0.2.2",
		"@streamr/test-utils": "^8.5.5",
		"@types/lodash": "^4.14.200",
		"@types/node": "^18.15.10",
		"@types/node-fetch": "^2.6.2",
		"@types/pino": "^7.0.5",
		"@types/websocket": "^1.0.5",
		"@types/ws": "^8.5.3",
		"del-cli": "^5.0.0",
		"dotenv": "^16.3.1",
		"esbuild": "^0.18.17",
		"esbuild-node-externals": "^1.8.0",
		"eslint": "^8.36.0",
		"eslint-plugin-immutable": "^1.0.0",
		"npm-run-all": "^4.1.5",
		"pkg": "^5.6.0",
		"resolve-tspaths": "^0.8.0",
		"strip-ansi": "^7.0.0",
		"ts-node": "^10.9.1",
		"ts-toolbelt": "^9.6.0",
		"tsconfig-paths": "^4.1.0",
		"tslib": "^2.5.0",
		"tsx": "^3.12.7",
		"typescript": "^4.9.5",
		"vite": "^4.4.8",
		"vite-tsconfig-paths": "^4.2.0",
		"vitest": "^0.34.1"
	}
}
